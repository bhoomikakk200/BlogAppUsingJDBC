<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
		<servlet-name>addblog</servlet-name>
		<servlet-class>com.AddBlogJDBC</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>deleteblog</servlet-name>
		<servlet-class>com.DeleteBlogJDBC</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>updateblog</servlet-name>
		<servlet-class>com.UpdateDatas</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>addblog</servlet-name>
		<url-pattern>/AddBlog1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>deleteblog</servlet-name>
		<url-pattern>/DeleteBlog1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>updateblog</servlet-name>
		<url-pattern>/UpdateBlogJDBC</url-pattern>
	</servlet-mapping>
</web-app>
